{
    "name": "python-dev-vscode",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "python-dev",
    "workspaceFolder": "/python-dev",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-vscode-remote.remote-containers",
          "ms-azuretools.vscode-docker"
        ]
      },
      "settings": {
        "python.pythonPath": "/opt/conda/envs/python-dev-env/bin/python"
      }
    }
}